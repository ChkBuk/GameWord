	public static void miniMaxSum(List<Integer> arr) {
	    // Write your code here
		int[] convertToArr = {};
		int i=0;
		for (int integer: arr) {
			convertToArr[i] = integer;
		}
		Arrays.sort(convertToArr);
	}

}
